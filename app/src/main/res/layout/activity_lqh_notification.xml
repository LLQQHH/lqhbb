<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homepager.LqhNotificationActivity"
    android:orientation="vertical">
    <TextView
        android:id="@+id/tv_notification"
        android:padding="@dimen/dp_20"
        android:text="点击通知"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView
        android:id="@+id/tv_notification1"
        android:padding="@dimen/dp_20"
        android:text="点击通知1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView
        android:id="@+id/tv_notification2"
        android:padding="@dimen/dp_20"
        android:text="点击通知2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView
        android:id="@+id/tv_notification3"
        android:padding="@dimen/dp_20"
        android:text="点击进度条通知"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"/>
    <TextView
        android:id="@+id/tv_notification4"
        android:padding="@dimen/dp_20"
        android:text="点击去通知设置1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"/>
</LinearLayout>